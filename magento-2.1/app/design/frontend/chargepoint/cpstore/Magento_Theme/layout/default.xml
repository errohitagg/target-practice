<body>
  <referenceBlock name="logo">
      <arguments>
          <argument name="logo_file" xsi:type="string">images/charge-point-logo-cmyk-pc.png</argument>
          <argument name="logo_img_width" xsi:type="number">126</argument>
          <argument name="logo_img_height" xsi:type="number">23.4</argument>
      </arguments>
  </referenceBlock>

  <referenceBlock name="top.links">
    <!-- <move element="navigation.sections" destination="header-wrapper" after="logo"/> -->
    <referenceBlock name="header.panel.wrapper" remove="true" />
    <referenceBlock name="top.search" remove="true" />
  </referenceBlock>
  <referenceBlock name="report.bugs" remove="true" />
  <referenceBlock name="footer_links" remove="true" />
  <referenceBlock name="form.subscribe" remove="true" />
  <referenceBlock name="copyright" remove="true" />
  <referenceContainer name="footer-container">
       <block class="Magento\Theme\Block\Html\Footer" name="footer" template="html/footer.phtml"/>
   </referenceContainer>
</body>
